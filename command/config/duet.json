{
    "task":{
        "name":"rank",
        "loss":[
            "rce"
        ],
        "metrics":[
            "pre",
            "ndcg"
        ]
    },
    "data_url":[
        "./data/train_data.csv",
        "./data/dev_data.csv",
        "./data/test_data.csv"
    ],
    "preprocessor":{
        "truncated_length_left":10,
        "truncated_length_right":100,
        "truncated_mode":"pre",
        "filter_mode":"df",
        "filter_low_freq":2,
        "filter_high_freq":1000,
        "remove_stop_words":true,
        "ngram_size":3
    },
    "Dataset":{
        "mode":"pair",
        "num_dup":2,
        "num_neg":1,
        "batch_size":32,
        "shuffle":true,
        "sort":false,
        "callbacks":null

    },
    "callback":{
        "fixed_length_left":10,
        "fixed_length_right":100,
        "pad_word_value":0,
        "pad_word_mode":"pre",
        "with_ngram":true,
        "fixed_ngram_length":null
    },
    "model":{
        "model_class":"duet",
        "task":null,
        "out_activation_func":null,
        "with_multi_layer_perceptron":true,
        "mlp_num_units":100,
        "mlp_num_layers":2,
        "mlp_num_fan_out":100,
        "mlp_activation_func":"relu",
        "mask_value":0,
        "left_length":10,
        "right_length":100,
        "lm_filters":100,
        "vocab_size":419,
        "dm_filters":100,
        "dm_kernel_size":3,
        "dm_conv_activation_func":"relu",
        "dm_right_pool_size":4,
        "dropout_rate":0.5
    },
    "device":null,
    "optimizer":"adadelta",
    "lr":0.01,
    "epoch":10
}